{
  "expo": {
    "name": "MyRush",
    "slug": "myrush",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.myrush.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.Unphuc.MYRUSH",
      "backgroundColor": "#000000",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Lexend_300Light.ttf",
            "./assets/fonts/Lexend_400Regular.ttf",
            "./assets/fonts/Lexend_500Medium.ttf",
            "./assets/fonts/Lexend_600SemiBold.ttf",
            "./assets/fonts/Lexend_700Bold.ttf",
            "./assets/fonts/Lexend_900Black.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "1bf0d3e3-07d4-47b5-b9e4-3054b892dac2"
      },
      "apiBaseUrl": "http://65.0.195.149:8000"
    }
  }
}